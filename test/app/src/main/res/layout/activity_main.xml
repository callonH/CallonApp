<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:hkl="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="nenu.callon.test.com.test.MainActivity">

    <nenu.callon.test.com.test.RoundProgressBarWidthNumber
        android:id="@+id/RoundProgressBar01"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:layout_marginLeft="100px"
        android:layout_marginTop="30dip"
        android:background="#00000000"
        android:progress="0"
        hkl:progress_text_color="#ff2903FC" />

    <nenu.callon.test.com.test.RoundProgressBarWidthNumber
        android:id="@+id/RoundProgressBar02"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:layout_marginLeft="100px"
        android:layout_toRightOf="@id/RoundProgressBar01"
        android:layout_marginTop="30dip"
        android:background="#00000000"
        android:progress="0"
        hkl:progress_text_color="#ffF53B03" />

    <nenu.callon.test.com.test.RoundProgressBarWidthNumber
        android:id="@+id/RoundProgressBar03"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:layout_marginLeft="100px"
        android:layout_toRightOf="@id/RoundProgressBar02"
        android:layout_marginTop="30dip"
        android:background="#00000000"
        android:progress="0"
        hkl:progress_text_color="#00cc00" />
    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/RoundProgressBar01"
        android:layout_marginLeft="115px"
        android:layout_marginTop="26dp"
        android:textColor="#ff2903FC"
        android:text="Diluent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView1"
        android:layout_alignTop="@+id/textView1"
        android:layout_marginLeft="135px"
        android:layout_toRightOf="@+id/textView1"
        android:textColor="#ffF53B03"
        android:text="Sample" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView2"
        android:layout_marginLeft="125px"
        android:layout_toRightOf="@+id/textView2"
        android:textColor="#00cc00"
        android:text="Injection" />

</RelativeLayout>
